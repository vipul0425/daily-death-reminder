{
    "name" : "Death Reminder",
    "version" : "1",
    "manifest_version" : 2,
    "browser_action" : {
        "default_popup" : "popup.html",
        "default_icon" : "/assets/images/logo.png"
    },
    "icons" : {
        "128" : "/assets/images/logo.png"
    },
    "background" : {
        "scripts": ["/assets/js/background.js"],
        "persistent": false
    },
    "permissions":["alarms","notifications"]
}